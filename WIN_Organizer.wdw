  procedures :
   -
     name : ChangeOrganizerMode
     procedure_id : 2059120408202112926
     type_code : 14
     code : |1-
      // Summary: Changes the mode of the organizer
      // Syntax:
      //OrganizerChangeMode (<eMode> is WIN_Home.EOrganizerMode)
      //
      // Parameters:
      //	eMode (WIN_Home.EOrganizerMode): The mode to display
      // Return value:
      // 	None
      //
      Procédure ChangeOrganizerMode(eMode is EOrganizerMode) <UI>
      
      BTN_Day..Pushed = False
      BTN_Week..Pushed = False
      BTN_Month..Pushed = False
      
      geOrganizerMode = eMode
      
      switch eMode
      	case OrganizerMode_Day
      		BTN_Day..Pushed = True
      		ORG_APT.ChangeMode(orgzDay)
      	case OrganizerMode_Week_5
      		BTN_Week..Pushed = True
      		ORG_APT.ChangeMode(orgzWeekOn5)
      	case OrganizerMode_Month
      		BTN_Month..Pushed = True
      		ORG_APT.ChangeMode(orgzMonth)
      		
      end
      
      RefreshOrganizerInfo()
     type : 458752
   -
     name : RefreshOrganizer
     procedure_id : 2059120408202178462
     type_code : 14
     code : |1-
      // Summary: Refreshes the organizer
      //
      Procédure RefreshOrganizer() 
      
      ORG_APT.DeleteAll()
      
      for each apt 
      	myapt is an Appointment
      	
      	myapt.Title = apt.Title
      	myapt.Content = apt.Content
      	myapt.StartDate = apt.StartDate
      	myapt.EndDate = apt.EndDate
      	
      	myapt.ID = apt.APTID
      	
      	// Color search
      	HReadSeek(aptbckgrd,APTBckgrdID,apt.APTBckgrdID)
      	myapt.BackgroundColor	= HTMLToRGB(aptbckgrd.BackgroundColor)
      	
      	ORG_APT.AddAppointment(myapt)
      end
     type : 458752
   -
     name : RefreshOrganizerInfo
     internal_properties : CAAAAAgAAABE/MVF5aMcL76pBEaXWlOMo74FA7G+zB3cXMB0Xw8mH65mtt7N/lxw9mTVQhIPIwmg8Y+KRdn5XJOmcuSc3mL0B7Oo9URtGWVkU7W5N1/3z8B77Ni077BPL5wX36RNiezmzeaWe65eGWmUn9Cxc7AZhOJoMe9gJzwxrn31ctq2rwrkFrYH9Fm0og+Rh1omPzvI3fC9ZHguVAWkuigjjigLYdoCkoIf
     procedure_id : 2059120408202243998
     type_code : 14
     code : |1-
      // Summary: Refreshes the organizer information
      //
      Procédure RefreshOrganizerInfo() <UI>
      
      dStart is date = OrganizerPosition(ORG_APT)
      
      
      
      switch geOrganizerMode
      	case OrganizerMode_Day
      		MyWindow..Title = "Organizer control"
      	case OrganizerMode_Week_5,OrganizerMode_Week_7
      		MyWindow..Title = "Week " + WeekNumber(dStart)		
      	case OrganizerMode_Month
      		<compile if ConfigurationType=Android>
      			MyWindow..Title = DateToString(OrganizerPosition(ORG_APT,orgCurrentMonth),"Mmmm")
      		<else>
      			MyWindow..Title = "Organizer control"
      		<end>
      end
     type : 458752
