   -
     code : |1+
      Procédure MyWindow()
      
      // Modes of the organizer
      EOrganizerMode is énumération
      	OrganizerMode_Day
      	OrganizerMode_Week_5
      	OrganizerMode_Week_7
      	OrganizerMode_Month
      end
      
      // Default mode
      geOrganizerMode is EOrganizerMode
      gSelectedAPT is an Appointment
      
      // If the application is displayed on a large screen (tablet or PC)
      if ScreenType() in (teTablet, teComputer) then
      	// Tablet
      	geOrganizerMode = OrganizerMode_Week_5
      else
      	// Phone
      	geOrganizerMode = OrganizerMode_Day
      end
      
      
      // Change the mode
      ChangeOrganizerMode(geOrganizerMode)
   -
     code : |1+
      
      // Initialize the database
      InitDB()
      
      // Fill the calendar
      RefreshOrganizer()
     type : 34
